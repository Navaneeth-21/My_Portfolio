{
  "version": 2,
  "builds": [
      {
          "src": "public/**",
          "use": "@vercel/static"
      },
      {
          "src": "api/contact.js",
          "use": "@vercel/node"
      }
  ],
  "routes": [
      {
          "src": "/api/contact",
          "dest": "/api/contact.js",
          "methods": ["POST"]
      },
      {
          "src": "/(.*)",
          "dest": "/public/$1",
          "headers": {
              "Access-Control-Allow-Origin": "*"
          }
      }
  ]
}